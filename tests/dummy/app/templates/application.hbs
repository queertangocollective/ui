<label for='text-field'>Text Field</label>
{{text-field inputId='text-field' value=text onchange=(action (mut text))}}
{{text}}

<label for='number-field'>Number Field</label>
{{number-field inputId='number-field' value=number onchange=(action (mut number))}}
{{number}}

<label for='percent-field'>Percent Field</label>
{{percent-field inputId='percent-field' value=percent onchange=(action (mut percent))}}
{{percent}}

<label for='password-field'>Password</label>
{{password-field inputId='password-field' value=password onchange=(action (mut password))}}
{{password}}

<label for='search-field'>Search</label>
{{search-field inputId='search-field' query=query onquery=(action (mut query))}}
{{query}}

<label for='auto-complete'>Autocomplete</label>
{{auto-complete inputId='auto-complete' value=autocomplete onchange=(action (mut autocomplete)) onquery=(action 'query') key='name'}}
{{autocomplete.alpha2Code}} / {{autocomplete.name}}

{{outlet}}
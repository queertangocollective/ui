<input type='text'
       id={{inputId}}
       name={{name}}
       placeholder={{placeholder}}
       readonly={{if readonly 'readonly'}}
       autocomplete="new-password"
       onfocus={{action 'focus'}}
       onblur={{action 'blur'}}
       onpaste={{action 'reformat'}}
       onchange={{action 'reformat'}}
       oninput={{action 'reformat'}}
       disabled={{disabled}}>